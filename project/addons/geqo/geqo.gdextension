windows.debug.x86_64 = "res://addons/geqo/bin/geqo.windows.template_debug.x86_64.dll"
windows.release.x86_64 = "res://addons/geqo/bin/geqo.windows.template_release.x86_64.dll"

linux.debug.x86_64 = "res://addons/geqo/bin/geqo.linux.template_debug.x86_64.so"
linux.release.x86_64 = "res://addons/geqo/bin/geqo.linux.template_release.x86_64.so"
linux.debug.arm64 = "res://addons/geqo/bin/geqo.linux.template_debug.arm64.so"
linux.release.arm64 = "res://addons/geqo/bin/geqo.linux.template_release.arm64.so"
linux.debug.rv64 = "res://addons/geqo/bin/geqo.linux.template_debug.rv64.so"
