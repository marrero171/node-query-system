[configuration]

entry_symbol = "geqo_library_init"
compatibility_minimum = "4.5"
reloadable = true

[icons]

CEnvironmentQuery = "res://addons/geqo/icons/environment_query.svg"
CQueryTest3D = "res://addons/geqo/icons/query_test3d.svg"
CQueryGenerator3D = "res://addons/geqo/icons/generator3d.svg"
CTestDistanceTo = "res://addons/geqo/icons/distance_test3d.svg"

[libraries]

macos.debug = "res://addons/geqo/bin/geqo.macos.template_debug.framework"
